<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Navigator">
	<test name="Temporary viewlets">
		<parameter name="sDriver" value="webdriver.chrome.driver" />
		<parameter name="sDriverpath" value="Drivers/chromedriver.exe" />
	   
		<parameter name="Dashboardname" value="Temp"/>


		<classes>
			<class name="ApodGUI.TemporaryViewlet">


				<methods>
					<include name="Login" />  
					<include name="CreateTemporaryViewletUsingDirectOption"/> 
					<include name="CreateTemporaryViewletUsingTemporaryViewletCheckbox"/>
					<include name="Logout" />
				</methods>

			</class>
		</classes>

	</test>
</suite> 